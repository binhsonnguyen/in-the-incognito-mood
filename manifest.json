{
   "description": "A dark theme that give mood similar to Chrome's Incognito mode",
   "manifest_version": 2,
   "name": "In the Incognito Mood",
   "theme": {
      "colors": {
         "frame": [ 40, 43, 45 ],
         "frame_inactive": [ 30, 30, 30 ],
         "toolbar": [ 80, 80, 80 ],
         "bookmark_text": [ 255, 255, 255 ],
         "tab_background_text": [ 255, 255, 255 ],
         "ntp_background": [ 48, 48, 48 ],
         "ntp_text": [ 255, 255, 255 ],
         "tab_text": [ 241, 243, 244 ]
      },
      "properties": {
         "ntp_background_repeat": "repeat",
         "ntp_logo_alternate": 1
      },
      "tints": {
         "buttons": [ -1, -1, 0.85 ],
         "frame": [ -1, 0.2, 0.35 ],
         "frame_inactive": [ -1, 0.3, 0.6 ]
      }
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}
